{
  "framework": null,
  "installCommand": "uv pip install --system -r requirements.txt",
  "buildCommand": "mkdir -p public && echo 'Dependencies installed'",
  "outputDirectory": "public",
  "rewrites": [
    { "source": "/", "destination": "/api/index" },
    { "source": "/((?!api/).*)", "destination": "/api/index/$1" }
  ]
}
